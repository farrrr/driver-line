{
    "name": "farlab/driver-line",
    "description": "BotMan driver to connect Line messenger with BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "Line Messenger",
        "Line",
        "Messenger"
    ],
    "homepage": "https://github.com/farlab/driver-line",
    "require": {
        "php": ">=7.0",
        "botman/botman": "~2.1|~3.0",
        "linecorp/line-bot-sdk": "^3.1"
    },
    "require-dev": {
        "botman/studio-addons": "~1.0",
        "phpunit/phpunit": "~5.0",
        "illuminate/contracts": "~5.5.0",
        "mockery/mockery": "dev-master",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "FarLab\\Drivers\\Line\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FarLab\\Drivers\\Line\\Providers\\LineServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Far Tseng",
            "email": "farlab@gmail.com"
        }
    ]
}
